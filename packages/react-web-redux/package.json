{
  "name": "@elux/react-web-redux",
  "version": "0.0.3",
  "description": "The practice of elux on react + web",
  "repository": "https://github.com/hiisea/elux/tree/master/packages/react-web-redux",
  "author": "hiisea <hiisea@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "jsnext:main": "dist/next/index.js",
  "types": "types/",
  "files": [
    "env/",
    "types/",
    "dist/",
    "lib/"
  ],
  "scripts": {
    "prebuild": "node ../../preBuild && rimraf ./dist ./types && tsc --build ./src",
    "build:next": "cross-env NODE_TAG=next babel ./src --out-dir ./dist/next  --extensions \".ts,.tsx\"",
    "build:esm": "cross-env NODE_TAG=esm babel ./src --out-dir ./dist/esm  --extensions \".ts,.tsx\"",
    "build:cjs": "cross-env NODE_TAG=cjs babel ./src --out-dir ./dist/cjs  --extensions \".ts,.tsx\"",
    "package:next": "cross-env NODE_TAG=next rollup -c",
    "package:esm": "cross-env NODE_TAG=esm rollup -c",
    "build": "yarn build:next & yarn build:esm & yarn build:cjs & yarn package:next && yarn package:esm"
  },
  "externals": {
    "react": true,
    "react-dom": true,
    "@elux/react-web": true
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@babel/runtime": "~7.12.18",
    "@elux/react-web": "^0.1.0",
    "react-redux": "^7.2.2"
  },
  "devDependencies": {
    "@types/react": "~17.0.0",
    "@types/react-dom": "~17.0.0",
    "react": "~17.0.0",
    "react-dom": "~17.0.0"
  },
  "gitHead": "7d6eb6d666143271ba0575fea99cf3f927fce764"
}
