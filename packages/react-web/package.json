{
  "name": "@elux/react-web",
  "version": "1.1.0",
  "description": "The practice of elux on react-web",
  "repository": "https://github.com/hiisea/elux/tree/master/packages/react-web",
  "author": "hiisea <hiisea@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/cjs/pkg.js",
  "module": "dist/esm/pkg.js",
  "jsnext:main": "dist/next/pkg.js",
  "types": "types/",
  "files": [
    "types/",
    "dist/"
  ],
  "scripts": {
    "eslint": "cross-env NODE_ENV=production eslint --fix --cache **/*.{js,ts}",
    "build:types": "rimraf ./types && tsc --build ./src",
    "build:next": "cross-env NODE_TAG=next babel ./src --out-dir ./dist/next  --extensions \".ts\"",
    "build:esm": "cross-env NODE_TAG=esm babel ./src --out-dir ./dist/esm  --extensions \".ts\"",
    "build:cjs": "cross-env NODE_TAG=cjs babel ./src --out-dir ./dist/cjs  --extensions \".ts\"",
    "package:next": "cross-env NODE_TAG=next rollup -c",
    "package:esm": "cross-env NODE_TAG=esm rollup -c",
    "api:json": "yarn build:types && api-extractor run",
    "build": "yarn api:json && rimraf ./dist && yarn build:next & yarn build:esm & yarn build:cjs & yarn package:next && yarn package:esm"
  },
  "externals": {
    "react": true,
    "react-dom": true
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17",
    "react-dom": "^16.8.0 || ^17"
  },
  "dependencies": {
    "@elux/app": "^1.1.0",
    "@elux/core": "^1.1.0",
    "@elux/react-components": "^1.1.0",
    "@elux/react-redux": "^1.1.0",
    "@elux/route": "^1.1.0",
    "@elux/route-browser": "^1.1.0"
  },
  "gitHead": "db6eb1314ef3e6abf5b7014495f441911c4cdd28"
}
