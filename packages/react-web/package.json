{
  "name": "@elux/react-web",
  "version": "1.0.0",
  "description": "The practice of elux on react + web",
  "repository": "https://github.com/hiisea/elux/tree/master/packages/react-web",
  "author": "hiisea <hiisea@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "jsnext:main": "dist/next/index.js",
  "types": "types/",
  "files": [
    "runtime/",
    "types/",
    "dist/"
  ],
  "scripts": {
    "eslint": "cross-env NODE_ENV=production eslint --fix --cache **/*.{js,ts}",
    "prebuild": "rimraf ./dist ./types && tsc --build ./src",
    "build:next": "cross-env NODE_TAG=next babel ./src --out-dir ./dist/next  --extensions \".ts\"",
    "build:esm": "cross-env NODE_TAG=esm babel ./src --out-dir ./dist/esm  --extensions \".ts\"",
    "build:cjs": "cross-env NODE_TAG=cjs babel ./src --out-dir ./dist/cjs  --extensions \".ts\"",
    "build": "yarn build:next & yarn build:esm & yarn build:cjs"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17",
    "react-dom": "^16.8.0 || ^17"
  },
  "dependencies": {
    "@elux/app": "^1.0.0",
    "@elux/core": "^1.0.0",
    "@elux/react-components": "^1.0.0",
    "@elux/route-browser": "^1.0.0"
  },
  "gitHead": "7d6eb6d666143271ba0575fea99cf3f927fce764"
}
