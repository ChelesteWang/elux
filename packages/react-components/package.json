{
  "name": "@elux/react-components",
  "version": "1.2.0",
  "description": "The practice of elux on react",
  "repository": "https://github.com/hiisea/elux/tree/master/packages/react-components",
  "author": "hiisea <hiisea@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "jsnext:main": "dist/next/index.js",
  "types": "types/",
  "files": [
    "runtime/",
    "types/",
    "dist/",
    "stage/"
  ],
  "scripts": {
    "eslint": "cross-env NODE_ENV=production eslint --fix --cache **/*.{js,ts,tsx}",
    "build:types": "rimraf ./types && tsc --build ./src",
    "build:next": "cross-env NODE_TAG=next babel ./src --out-dir ./dist/next  --extensions \".ts,.tsx\"",
    "build:esm": "cross-env NODE_TAG=esm babel ./src --out-dir ./dist/esm  --extensions \".ts,.tsx\"",
    "build:cjs": "cross-env NODE_TAG=cjs babel ./src --out-dir ./dist/cjs  --extensions \".ts,.tsx\"",
    "build": "yarn build:types && rimraf ./dist && yarn build:next & yarn build:esm & yarn build:cjs"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17",
    "react-dom": "^16.8.0 || ^17"
  },
  "dependencies": {
    "@elux/core": "^1.2.0",
    "@elux/route": "^1.2.0"
  },
  "gitHead": "db6eb1314ef3e6abf5b7014495f441911c4cdd28"
}
