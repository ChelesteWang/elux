{
  "name": "@elux/core",
  "version": "0.1.56",
  "description": "Stateful and Modular for Front-End Engineering, Suitable for various javascript runtime ",
  "homepage": "https://github.com/hiisea/elux/tree/master/packages/core",
  "author": "hiisea <hiisea@qq.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "vue",
    "redux",
    "vuex",
    "typescript",
    "state container",
    "state management"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "jsnext:main": "dist/next/index.js",
  "types": "types/",
  "files": [
    "runtime/",
    "types/",
    "dist/"
  ],
  "scripts": {
    "eslint": "cross-env NODE_ENV=production eslint --fix --cache **/*.{js,ts} --ignore-pattern **/*.d.ts",
    "prebuild": "node ../../preBuild && rimraf ./dist ./types && tsc --build ./src",
    "build:next": "cross-env NODE_TAG=next babel ./src --out-dir ./dist/next  --extensions \".ts\"",
    "build:esm": "cross-env NODE_TAG=esm babel ./src --out-dir ./dist/esm  --extensions \".ts\"",
    "build:cjs": "cross-env NODE_TAG=cjs babel ./src --out-dir ./dist/cjs  --extensions \".ts\"",
    "build:ts": "tsc -P ./src --module ESNext --isolatedModules false --noEmit false --outDir ./dist/ts",
    "build": "yarn build:next & yarn build:esm & yarn build:cjs"
  },
  "dependencies": {
    "@babel/runtime": "~7.12.18"
  },
  "gitHead": "7d6eb6d666143271ba0575fea99cf3f927fce764"
}
