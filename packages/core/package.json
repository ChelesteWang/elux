{
  "name": "@elux/core",
  "version": "1.2.0",
  "description": "Stateful and Modular for Front-End Engineering, Suitable for various javascript runtime ",
  "homepage": "https://github.com/hiisea/elux/tree/master/packages/core",
  "author": "hiisea <hiisea@qq.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "vue",
    "redux",
    "vuex",
    "typescript",
    "state container",
    "state management"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "jsnext:main": "dist/next/index.js",
  "types": "types/",
  "files": [
    "runtime/",
    "types/",
    "dist/"
  ],
  "scripts": {
    "test": "jest",
    "eslint": "cross-env NODE_ENV=production eslint --fix --cache **/*.{js,ts}",
    "build:types": "rimraf ./types && tsc --build ./src",
    "build:next": "cross-env NODE_TAG=next babel ./src --out-dir ./dist/next  --extensions \".ts\"",
    "build:esm": "cross-env NODE_TAG=esm babel ./src --out-dir ./dist/esm  --extensions \".ts\"",
    "build:cjs": "cross-env NODE_TAG=cjs babel ./src --out-dir ./dist/cjs  --extensions \".ts\"",
    "build": "yarn build:types && rimraf ./dist && yarn build:next && yarn build:esm && yarn build:cjs"
  },
  "gitHead": "db6eb1314ef3e6abf5b7014495f441911c4cdd28"
}
