{
  "name": "@elux/react-redux-taro",
  "version": "0.1.12",
  "description": "The practice of elux on react-redux-taro",
  "repository": "https://github.com/hiisea/elux/tree/master/packages/react-redux-taro",
  "author": "hiisea <hiisea@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/cjs/pkg.js",
  "module": "dist/esm/pkg.js",
  "jsnext:main": "dist/next/pkg.js",
  "types": "types/",
  "files": [
    "types/",
    "dist/"
  ],
  "scripts": {
    "eslint": "cross-env NODE_ENV=production eslint --fix --cache **/*.{js,ts} --ignore-pattern **/*.d.ts",
    "prebuild": "node ../../preBuild && rimraf ./dist ./types && tsc --build ./src",
    "build:next": "cross-env NODE_TAG=next babel ./src --out-dir ./dist/next  --extensions \".ts\"",
    "build:esm": "cross-env NODE_TAG=esm babel ./src --out-dir ./dist/esm  --extensions \".ts\"",
    "build:cjs": "cross-env NODE_TAG=cjs babel ./src --out-dir ./dist/cjs  --extensions \".ts\"",
    "build:ts": "tsc -P ./src --module ESNext --isolatedModules false --noEmit false --outDir ./dist/ts",
    "package:next": "cross-env NODE_TAG=next rollup -c",
    "package:esm": "cross-env NODE_TAG=esm rollup -c",
    "build": "yarn build:next & yarn build:esm & yarn build:cjs & yarn package:next && yarn package:esm"
  },
  "externals": {
    "@tarojs/taro": true,
    "react": true,
    "react-dom": true
  },
  "peerDependencies": {
    "@tarojs/taro": "^3.3.1",
    "react": "^16.8.0 || ^17"
  },
  "dependencies": {
    "@babel/runtime": "~7.12.18",
    "@elux/react-redux": "^0.1.12",
    "@elux/react-taro": "^0.1.12"
  },
  "gitHead": "7d6eb6d666143271ba0575fea99cf3f927fce764"
}
