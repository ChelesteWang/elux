{
  "name": "@elux/react-taro",
  "version": "0.1.2",
  "description": "The practice of react + taro",
  "repository": "https://github.com/hiisea/elux/tree/master/packages/react-taro",
  "author": "hiisea <hiisea@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "jsnext:main": "dist/next/index.js",
  "types": "types/",
  "files": [
    "runtime/",
    "types/",
    "dist/"
  ],
  "scripts": {
    "eslint": "cross-env NODE_ENV=production eslint --fix --cache **/*.{js,ts,tsx} --ignore-pattern **/*.d.ts",
    "prebuild": "node ../../preBuild && rimraf ./dist ./types && tsc --build ./src",
    "build:next": "cross-env NODE_TAG=next babel ./src --out-dir ./dist/next  --extensions \".ts,.tsx\"",
    "build:esm": "cross-env NODE_TAG=esm babel ./src --out-dir ./dist/esm  --extensions \".ts,.tsx\"",
    "build:cjs": "cross-env NODE_TAG=cjs babel ./src --out-dir ./dist/cjs  --extensions \".ts,.tsx\"",
    "build:ts": "tsc -P ./src --module ESNext --isolatedModules false --noEmit false --outDir ./dist/ts",
    "build": "yarn build:next & yarn build:esm & yarn build:cjs"
  },
  "peerDependencies": {
    "@tarojs/taro": "^3.3.1",
    "react": "^16.8.0 || ^17",
    "react-dom": "^16.8.0 || ^17"
  },
  "dependencies": {
    "@babel/runtime": "~7.12.18",
    "@elux/core": "^0.1.2",
    "@elux/route-mp": "^0.1.2"
  },
  "devDependencies": {
    "@types/react": "~17.0.0",
    "@types/react-dom": "~17.0.0",
    "react": "~17.0.0",
    "react-dom": "~17.0.0"
  },
  "gitHead": "c2d8bb9ed8d10102413bdf655c7326e94f4a4464"
}
