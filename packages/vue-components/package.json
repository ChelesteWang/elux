{
  "name": "@elux/vue-components",
  "version": "1.0.0",
  "description": "The practice of elux on vue",
  "repository": "https://github.com/hiisea/elux/tree/master/packages/vue-components",
  "author": "hiisea <hiisea@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "jsnext:main": "dist/next/index.js",
  "types": "types/",
  "files": [
    "runtime/",
    "types/",
    "dist/",
    "stage/"
  ],
  "scripts": {
    "eslint": "cross-env NODE_ENV=production eslint --fix --cache **/*.{js,ts,tsx}",
    "prebuild": "rimraf ./dist ./types && tsc --build ./src",
    "build:next": "cross-env NODE_TAG=next babel ./src --out-dir ./dist/next  --extensions \".ts,.tsx\"",
    "build:esm": "cross-env NODE_TAG=esm babel ./src --out-dir ./dist/esm  --extensions \".ts,.tsx\"",
    "build:cjs": "cross-env NODE_TAG=cjs babel ./src --out-dir ./dist/cjs  --extensions \".ts,.tsx\"",
    "build": "yarn build:next & yarn build:esm & yarn build:cjs"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@elux/core": "^1.0.0",
    "@elux/route": "^1.0.0",
    "@vue/server-renderer": "^3.0.0",
    "stream": "^0.0.2"
  },
  "gitHead": "7d6eb6d666143271ba0575fea99cf3f927fce764"
}
